<Window x:Class="OracleDbClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OracleDbClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Log in" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="149" Height="24" Click="Login_Button_Click"/>
        <TextBox x:Name="userName" HorizontalAlignment="Left" Height="24" Margin="164,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="248"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="417,10,0,0" VerticalAlignment="Top" Width="218" Height="24"/>
    </Grid>
</Window>
